<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:designPath="/libs/settings/wcm/designs/default"
        cq:lastModified="{Date}2020-12-01T13:14:10.132+01:00"
        cq:lastModifiedBy="admin"
        cq:template="/libs/cq/workflow/templates/model"
        jcr:primaryType="cq:PageContent"
        jcr:title="Mediahub - External publish"
        sling:resourceType="cq/workflow/components/pages/model">
        <flow
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <dmimageprocess_copy
                jcr:description="Processes assets for use with Dynamic Media Image Services"
                jcr:primaryType="nt:unstructured"
                jcr:title="Dynamic Media Process Image Assets"
                sling:resourceType="dam/components/s7imaging/dmimageprocess">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    CONFIGS="[140:100:false,48:48:false,319:319:false]"
                    HEIGHT="1280"
                    MIME_TYPE="image/jpeg"
                    PROCESS="com.adobe.cq.dam.dm.process.workflow.DMImageProcess"
                    PROCESS_AUTO_ADVANCE="true"
                    QUALITY="90"
                    WIDTH="1280"/>
            </dmimageprocess_copy>
            <scene7upload_copy
                jcr:description="Upload assets to configured remote Scene7 service for Dynamic Media delivery"
                jcr:primaryType="nt:unstructured"
                jcr:title="Scene7"
                sling:resourceType="dam/components/scene7/scene7upload">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    advanceOnTimeout="true"
                    EXTERNAL_PROCESS="com.day.cq.dam.scene7.impl.process.Scene7UploadProcess"
                    period="30"
                    PROCESS_AUTO_ADVANCE="true"
                    timeout="3600"/>
            </scene7upload_copy>
            <process_1607596535
              jcr:primaryType="nt:unstructured"
              jcr:title="Scene7 Asset Metadata"
              sling:resourceType="cq/workflow/components/model/process">
                <metaData
                  jcr:primaryType="nt:unstructured"
                  PROCESS="com.mediahub.core.workflows.SaveScene7MetadataProcess"
                  PROCESS_AUTO_ADVANCE="true"/>
            </process_1607596535>
            <process
              jcr:description="CDN Cache Invalidation"
              jcr:primaryType="nt:unstructured"
              jcr:title="CDN Cache Invalidation"
              sling:resourceType="cq/workflow/components/model/process">
                <metaData
                  jcr:primaryType="nt:unstructured"
                  PROCESS="com.mediahub.core.workflows.CdnInvalidateCacheWorkflowProcess"
                  PROCESS_AUTO_ADVANCE="true"/>
            </process>
        </flow>
    </jcr:content>
</jcr:root>
