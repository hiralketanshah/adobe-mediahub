<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <tabs
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <tab1
                granite:rel="aem-assets-metadata-form-tab"
                jcr:primaryType="nt:unstructured"
                jcr:title="Description"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                listOrder="0">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    tabid="f84cc8d6-60ab-474e-bdf8-a0b7a434f289"/>
                <items jcr:primaryType="nt:unstructured">
                    <col1
                        granite:rel="aem-assets-metadata-form-column"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        listOrder="0"/>
                    <col2
                        granite:rel="aem-assets-metadata-form-column"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        listOrder="1"/>
                    <col3
                        granite:rel="aem-assets-metadata-form-column"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        listOrder="2">
                        <items jcr:primaryType="nt:unstructured">
                            <size
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Size"
                                name="./jcr:content/metadata/size">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </size>
                            <videoDuration
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Duration (seconds)"
                                name="./jcr:content/metadata/videoDuration">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </videoDuration>
                            <bitrate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Total Bit Rate (kbps)"
                                name="./jcr:content/metadata/bitrate">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </bitrate>
                            <videoCodec
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Video Codec"
                                name="./jcr:content/metadata/videoCodec">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </videoCodec>
                            <videoBitrate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Video Bit Rate (kbps)"
                                name="./jcr:content/metadata/videoBitrate">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </videoBitrate>
                            <frameRate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Video Frame Rate (fps)"
                                name="./jcr:content/metadata/frameRate">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </frameRate>
                            <audioCodec
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Audio Codec"
                                name="./jcr:content/metadata/audioCodec">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </audioCodec>
                            <audioBitrate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Audio Bit Rate (kbps)"
                                name="./jcr:content/metadata/audioBitrate">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </audioBitrate>
                            <audioSampleRate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Audio Sample Rate (hz)"
                                name="./jcr:content/metadata/audioSampleRate">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </audioSampleRate>
                            <audioChannels
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}true"
                                fieldLabel="Number Audio Channels"
                                name="./jcr:content/metadata/audioChannels">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    metaType="text"/>
                            </audioChannels>
                        </items>
                    </col3>
                </items>
            </tab1>
        </items>
    </tabs>
</jcr:root>
