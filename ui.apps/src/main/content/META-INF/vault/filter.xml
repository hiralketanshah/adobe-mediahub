<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/apps/mediahub">
        <exclude pattern="/apps/mediahub/i18n"/>
    </filter>
    <filter root="/apps/sling"/>
    <filter root="/apps/granite"/>
    <filter root="/apps/cq">
        <exclude pattern="/apps/cq/core/content/nav"/>
    </filter>
    <filter root="/apps/clientlibs">
        <exclude pattern="/apps/clientlibs/social"/>
    </filter>
    <filter root="/apps/dam/temp" mode="merge">
        <exclude pattern="/apps/dam/temp/mediahub-medias-schema"/>
        <include pattern="/apps/dam/temp/rep:policy"/>
    </filter>
    <filter root="/apps/dam">
        <exclude pattern="/apps/dam/config"/>
        <exclude pattern="/apps/dam/temp"/>
    </filter>

    <filter root="/apps/settings/dam"/>
</workspaceFilter>
