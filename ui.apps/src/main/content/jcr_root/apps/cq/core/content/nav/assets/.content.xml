<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="nt:unstructured">
    <stocksearch
        jcr:primaryType="nt:unstructured">
        <granite:rendercondition
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <stockfeature
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/renderconditions/feature"
                feature="com.adobe.dam.asset.stock.feature.flag"/>
            <stockaccessible
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/gui/coral/components/commons/renderconditions/stockaccessible"/>
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-administrators,mediahub-basic-entity-manager]"/>
        </granite:rendercondition>
    </stocksearch>
    <medialibrary
            jcr:description="BNPP Medialibrary"
            jcr:primaryType="nt:unstructured"
            jcr:title="Medialibrary"
            desktopOnly="/damadmin#"
            desktopOnlyTitle="Classic UI"
            href="/assets.html/content/dam/medialibrary"
            icon="folder"
            id="cq-assets-files-medialibrary">
    </medialibrary>
    <projects
            jcr:description="BNPP Projects"
            jcr:primaryType="nt:unstructured"
            jcr:title="Projects"
            desktopOnly="/welcome"
            desktopOnlyTitle="Classic UI"
            href="/projects.html/content/projects"
            icon="project"
            id="cq-projects-bnpp">
    </projects>
    <photostock
            jcr:description="BNPP Photostock"
            jcr:primaryType="nt:unstructured"
            jcr:title="Photostock"
            desktopOnly="/damadmin#"
            desktopOnlyTitle="Classic UI"
            href="#"
            icon="folder"
            id="cq-assets-files-photostock">
        <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-administrators,mediahub-basic-entity-manager]"/>
        </granite:rendercondition>
    </photostock>
    <files
        jcr:primaryType="nt:unstructured">
        <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-super-administrators]"/>
        </granite:rendercondition>
    </files>
    <collections
        jcr:primaryType="nt:unstructured">
    </collections>
    <links
        jcr:primaryType="nt:unstructured">
        <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <damshare
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                    path="/var/dam/share"
                    privileges="[jcr:read]"/>
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-super-administrators]"/>
        </granite:rendercondition>
    </links>
    <templates
        jcr:mixinTypes="[rep:AccessControllable]"
        jcr:primaryType="nt:unstructured">
        <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-super-administrators]"/>
        </granite:rendercondition>
    </templates>
    <catalogs
        jcr:mixinTypes="[rep:AccessControllable]"
        jcr:primaryType="nt:unstructured">
        <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-super-administrators]"/>
        </granite:rendercondition>
    </catalogs>
    <asyncjobs
        jcr:primaryType="nt:unstructured">
        <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
            <groups
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/mediahub/components/renderconditions/groups"
                    groups="[administrators,mediahub-super-administrators]"/>
        </granite:rendercondition>
    </asyncjobs>
</jcr:root>
